<h1>Listing iterations</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Points completed</th>
      <th>Sprint</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @iterations.each do |iteration| %>
      <tr>
        <td><%= iteration.date %></td>
        <td><%= iteration.points_completed %></td>
        <td><%= iteration.sprint_id %></td>
        <td><%= link_to 'Show', iteration %></td>
        <td><%= link_to 'Edit', edit_iteration_path(iteration) %></td>
        <td><%= link_to 'Destroy', iteration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Iteration', new_iteration_path %>
